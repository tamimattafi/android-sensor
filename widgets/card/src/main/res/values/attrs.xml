<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SensibleImageCardView">
        <attr name="shadowRadius" format="integer" />
        <attr name="shadowColorFilter" format="color"/>
        <attr name="shadowAlpha" format="integer"/>
        <attr name="shadowBlurSampling" format="integer"/>
        <attr name="shadowPadding" format="dimension"/>
        <attr name="sensibleElement" format="enum">
            <enum name="shadow" value="0"/>
            <enum name="image" value="1" />
            <enum name="all" value="2"/>
            <enum name="none" value="3"/>
        </attr>
    </declare-styleable>
</resources>